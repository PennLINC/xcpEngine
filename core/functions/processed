processed() {
   local processed=${1}[${cxt}]
   if is_image ${!processed}
      then
      echo "${1}_space[${sub}]=${space[sub]}" >> ${design[sub]}
      echo "${1}[${sub}]=${!processed}"       >> ${design[sub]}
      rm -f ${out}/${prefix}.nii.gz
      exec_sys ln -s ${!processed} ${out}/${prefix}.nii.gz
   fi
}
