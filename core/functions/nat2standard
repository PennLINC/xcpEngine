nat2standard() {
   local i=${1}
   local o=${2}
   local n=${3}
   exec_sys rm -f ${o}
   exec_ants \
      antsApplyTransforms \
      -e 3 -d 3 \
      -i ${i} \
      -o ${o} \
      -r ${template} \
      ${resample} \
      ${warp} \
      ${affine} \
      ${rigid} \
      ${coreg} \
      -n ${n}
}
